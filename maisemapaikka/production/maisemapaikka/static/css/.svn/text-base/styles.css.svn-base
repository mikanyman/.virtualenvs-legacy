body {
	margin: 0;
	padding: 0;
	background: url("images/noise_pattern.png") repeat scroll 0 0
	/* background-color: #3D3D3D; */
	/*
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr=#000000, endColorstr=#ffffff);
	background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#000000), to(#fff)) no-repeat;
	background: -moz-linear-gradient(top, #000000, #fff) no-repeat;
	*/
	}
h1 {
	font-size: 16pt;
	color: #333333;
	margin-top: 0;
	}
h2 {
	font-size: 13pt;
	font-weight: bold;
	color: #0A3270;
	padding: 0;
	margin: 0;
	}
h3 {
	font-size: 12pt;
	font-weight: bold;
	}

/* Text spans */

.section_header {
	font-family: Verdana,Geneva,Arial,Helvetica,sans-serif;
	font-size: 14pt;
	font-weight: bold;
	line-height: normal;
	color: #FFFFFF;
	padding: 0;
	margin: 0;
	}
.section_aspect {
	font-family: Verdana,Geneva,Arial,Helvetica,sans-serif;
	font-size: 14pt;
	font-weight: bold;
	line-height: normal;
	color: #FFFFFF;
	padding: 0;
	margin: 0;
	}

/* Layout */

#top {
	background-color: #0A3270; /* 003366 */
	width: auto;
	height: 40px;
	border-bottom: solid 1px #EFEFEF; /* FFCC33 */
}
#topbanner {
	width: 100%;
	height: 300px;
	background-color: #FFFFFF; /* #00377B */
	font-family: Arial,Helvetica Neue,Helvetica,sans-serif;
	font-size: 14px;
	color: #FFCC33;
}
#logo {
	float: left;
	font-family: Palatino, Verdana, Arial,Helvetica Neue,Helvetica,sans-serif;
	font-size: 20pt;
	font-weight: bold;
	color: #FFCC33;
	margin: 2px 10px 0px 10px;
}
#slogan {
	float: left;
	font-family: Arial,Helvetica Neue,Helvetica,sans-serif;
	font-size: 20pt;
	font-weight: normal;
	color: #EFEFEF;
	margin-top: 2px;
}

#login {
	float: right;
	font-family: Arial,Helvetica Neue,Helvetica,sans-serif;
	font-size: 13pt;
	font-weight: normal;
	margin-top: 10px;
	margin-right: 10px;
	color: #FFFFFF;
	}
#login a {
	color: #FFFFFF;
	text-decoration: none;
	}
#login a:link {
	color: #FFFFFF;
	}
#login a:visited {
	color: #FFFFFF;
	}
#login a:hover {
	color: #FFCC33;
	}
#login a:active  {
	color: #FFCC33;
	}
#login .selected {
	color: #FFCC33;
	font-weight: bold;
	}
	
#flags {
	float: right;
	margin-top: 10px;
	margin-right: 10px;
	}
#flags img {
	border-style: none;
	}
#wrapper {
	width: 980px; /* 1160 */
	margin: 0 auto;
	background-color: #FFFFFF;
	border: solid 1px #666666;
}
.container {
	width: auto;
	background-color: #FFFFFF;
}
.clearfix {
	display: inline-block;
	}
#mid_container {
	background: url("images/navicol.gif") repeat-y scroll right top #FFFFFF;
	min-height: 600px;
	margin: 0;
	padding: 0;
	}
#main_col_header {
	width: 600px;
	height: 40px;
	background-color: #E02B26;
	color: #FFFFFF;
	font-family: Verdana,Geneva,Arial,Helvetica,sans-serif;
	font-size: 14pt;
	font-weight: bold;
	margin: 10px 0px 10px 10px;
	}
#main_col {
	float: left;
	width: 636px;
	min-height: 400px;
	}
#navi_col {
	width: 342px;
	float: right;
	font: 500 16px/19px "ff-clan-web-1",Helvetica,sans-serif,Arial;
	/* font-family: Verdana,Geneva,Arial,Helvetica,sans-serif;
	font-size: 10.5pt;
	line-height: normal;*/
	margin: 0;
	padding: 0;
	border: 0;
	}
#clearer {
	width: auto;
	clear: both;
	margin: 0;
	padding: 0;
	}
#footer {
	width: auto;
	height: 80px;
	background-color: #031E49;
	font-family: Arial, Helvetica, sans-serif;
	font-size: 12pt;
	line-height: 14pt;
	}
#textfooter_textblock {
	width: 980px;
	color: #FFCC33; /* EDEDED */
	padding: 10px;
	}

