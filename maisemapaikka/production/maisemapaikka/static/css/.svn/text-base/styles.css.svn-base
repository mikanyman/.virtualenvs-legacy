/* Table */

table#gallery {
	/* border: solid 1px #333; */
}
table#gallery td.img {
	width: 150px;
	height: 100px;
	background-color: #EFEFEF;
}
table#gallery td.txt {
	width: 150px;
	height: 20px;
	background-color: #FFF;
	font-size: 12px;
}

table#image {
	border: solid 1px #333;
}
table#image td {
	width: 750px;
	height: 360px;
	background-color: #EFEFEF;
}

table#form th td {
	font-weight: normal;
	text-align: right;
	}

/* arrows & buttons */
	
#tabs_1 {
	float: right;
	margin-right: 100px;
	position: relative;
	top: -10px;
	}
#tabs_2 {
	float: right;
	margin-right: 100px;
	position: relative;
	top: -10px;
	}
#page_name {
	width: 200px;
	height: 40px;
	float: left;
	padding: 10px 0px 0px 0px;
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 24pt;
	font-weight: bold;
	color: #2580A2;
	vertical-align: bottom;
	margin-left: 100px;
	}
#image_text {
	padding-top: 20px;
	}
#map {
	width: 500px;
	height: 300px;
	}
#form_table th {
	font-size: 12pt;
	font-weight: normal;
	text-align: right;
	vertical-align: top;
	}
.helptext {
	font-size: 10pt;
	}
.submit {
	margin-top: 20px;	
	}
.meta {
	font-size: 10pt;
	color: #666;
	margin-top: 20px;
	}
.padding_10 {
	padding: 10px;
	}
.padding_20 {
	padding: 10px;
	}
.padding_thumbs {
	margin: 28px 0px 28px 28px;
	}
.comment {
	font-size: 10px;
	color: #333;
	margin-bottom: 20px;
	}

/* form error handling */
td.error {
	font-size: 11pt;
	padding: 2px 20px;
	text-align: left;
	color: #CC0000;
}
td.error ul.errorlist {
	margin: 0;
	padding: 0;
}
td.error ul.errorlist li {
	display: inline;
	list-style-type: none;
	margin: 0;
	padding: 0;
}
.metadata {
    width:auto;
    border:solid 1px #EFEFEF;
    margin:10px 10px 0px 10px;
    padding:10px;
    background-color:#EFEFFF;
    font-family: Verdana, Geneva, Arial, Helvetica, sans-serif;
    font-size: 10.5pt;
    line-height: normal;
    color: #333333;
}

