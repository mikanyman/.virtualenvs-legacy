/* ul with arrows blue */

.navlist {
	margin: 0;
	padding: 0;
	list-style: none;
	}
.navlist a {
	font: 500 16px/19px "ff-clan-web-1",Helvetica,sans-serif,Arial;
	text-decoration: none;	
	}
.navlist li {
	background-image: url(images/arrow_red.gif);
	background-repeat: no-repeat;
	background-position: 0 1px;
	padding-left: 20px;
	}
.navlist a:link {
	color: #003366 !important;
	}
.navlist a:visited {
	color: #003366 !important;
	}
.navlist a:hover {
	color: #E02B26 !important;
	}
.navlist a:active  {
	color: #E02B26 !important;
	}
.navlist .selected {
	color: #E02B26 !important;
	font-weight: bold;
	}
	
