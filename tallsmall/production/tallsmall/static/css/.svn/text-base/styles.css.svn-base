	-moz-border-right-colors: none;
	-moz-border-top-colors: none;
	background: url("images/paper-crumpled-sm.jpg") repeat scroll 0 0 #FAE8D4;
	border-color: #F5F7F2 #C6BBA6 #BCB19C #F5F7F2;
	border-radius: 2px 2px 2px 2px;
	border-style: solid;
	border-width: 1px;
	box-shadow: 0 2px 6px rgba(0, 0, 0, 0.2);
	padding: 10px;
}
.sticky .entry-utility {
	background: none repeat scroll 0 0 rgba(198, 180, 153, 0.5);
}
