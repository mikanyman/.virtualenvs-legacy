* {
	margin: 0;
	padding: 0;
}
html, body {
	height: 100%;
	width: 100%;
}
body {
	font-family: Arial,Helvetica,sans-serif;
	font-size: 14px;
	line-height: 1.25;
}
body.custom-background {
	background-image: url('images/mp-background-tile.jpg');
	background-repeat: repeat;
	background-position: top center;
	background-attachment: fixed;
	}
img {
	border: 0 none;
}
font {
	font-family: Tahoma,Geneva,sans-serif;
}
h1, h2, h3, h4, h5, h6, .h1, .h2, .h3, .h4, .h5, .h6 {
	color: #603913;
	/* font-family: 'VollkornRegular',Geneva,sans-serif; */
	font-family: Arial, Helvetica, sans-serif;
	font-size: 40px;
	font-style: normal;
	font-variant: normal;
	letter-spacing: -2px;
	margin: 0;
	padding: 0;
}
h1, .h1 {
	font-weight: normal;
}
h2, .h2 {
	font-size: 28px;
	letter-spacing: -1px;
}
h3, .h3 {
	font-size: 20px;
	letter-spacing: -0.5px;
}
h4, .h4 {
	font-family: Arial,Helvetica,sans-serif;
	font-size: 18px;
	letter-spacing: 0;
}
h5, .h5 {
	font-family: Arial,Helvetica,sans-serif;
	font-size: 16px;
	letter-spacing: 0;
}
h6, .h6 {
	font-family: Arial,Helvetica,sans-serif;
	font-size: 14px;
	letter-spacing: 0;
}
p {
	line-height: 1.5em;
	margin-bottom: 1em;
}
ul, ol {
	margin-bottom: 1em;
	padding: 0;
}
ul ul {
	margin-bottom: 0;
	padding-bottom: 0;
}
li {
	margin-bottom: 0.5em;
	margin-left: 2.8em;
	padding: 0;
}
ul li {
	list-style: square outside none;
}
ul ul li {
	list-style: disc outside none;
}
ul ul ul li {
	list-style: circle outside none;
}
ol {
	margin-bottom: 1em;
}
ol li {
	list-style: decimal outside none;
}
ol ol li {
	list-style: upper-alpha outside none;
}
ol ol ol li {
	list-style: lower-roman outside none;
}
table {
	border-collapse: collapse;
	border-spacing: 0;
	font-size: 14px;
	width: 100%;
}
th {
	padding: 4px;
	text-align: left;
	vertical-align: top;
}
td {
	padding: 4px;
	text-align: left;
	vertical-align: top;
}
form {
	display: inline;
}
fieldset {
	padding: 10px;
}
input, textarea, select {
	border: 1px solid #CCCCCC;
	border-radius: 5px 5px 5px 5px;
	font-family: Arial,Helvetica,sans-serif;
	margin-bottom: 3px;
	padding: 4px;
}
textarea {
	width: 98%;
}
input.button {
	color: #333333;
	font-size: 16px;
}
input.button:hover {
	background-color: #660000;
	color: #FFFFFF;
}
input.button:active {
	background-color: #F3E9E9;
	color: #000000;
}
input:hover, textarea:hover, select:hover {
	border: 1px solid #660000;
}
input:focus, textarea:focus {
	background: none repeat scroll 0 0 #F3E9E9;
	border: 1px solid #660000;
}
select {
}
option {
}
pre {
	background-color: #F5F5F5;
	font-family: "Courier 10 Pitch",Courier,monospace;
	margin-bottom: 10px;
	padding: 10px;
}
blockquote {
	font-size: 16px;
	font-style: italic;
	padding: 7px 7px 0 50px;
}
hr, .hr {
