<!DOCTYPE html>
<html dir="ltr" lang="en-US">
<head>
  <meta http-equiv="X-UA-Compatible" content="IE=EmulateIE7" >
  <meta name="robots" content="noindex, nofollow">
  <link type="text/css" rel="stylesheet" media="screen" href="{{ STATIC_URL }}css/styles.css?{% now "YmdHis" %}"/>
  <!-- django-cms -->
  {% render_block "css" %}
   
  <!-- jQuery UI datepicker -->
  <link type="text/css" href="{{ STATIC_URL }}css/redmond/jquery-ui-1.8.17.custom.css" rel="stylesheet" />  
  <script type="text/javascript" src="{{ STATIC_URL }}js/jquery-1.7.1.min.js"></script>
  <script type="text/javascript" src="{{ STATIC_URL }}js/jquery-ui-1.8.17.custom.min.js"></script>
  <script type="text/javascript" src="{{ STATIC_URL }}js/jquery.ui.datepicker-{{ lang }}.js"></script>
  
  <!-- time picker add-on -->
  <script type="text/javascript" src="{{ STATIC_URL }}js/jquery-ui-timepicker-addon.js"></script>
  <script type="text/javascript" src="{{ STATIC_URL }}js/jquery-ui-sliderAccess.js"></script>
  
  <script type="text/javascript">
  $(function(){
    $('.datepicker').datepicker({
      dateFormat:'yy-mm-dd',
      showOn: 'button',
      buttonImage: '{{ STATIC_URL }}img/calendar.gif',
      buttonText: '{% trans "Pick Date" %}',
      buttonImageOnly: true
      },
    $.datepicker.regional['{{ lang }}']);
  });
  </script>
  <script type="text/javascript" src="{{ STATIC_URL }}js/pricecalc.js"></script>

</head>
